<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="admin-1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <uiComponent name="sunarc_splitorderpro_splitattr_form"/>
            <block class="Magento\Framework\View\Element\Template" name="splitorderproForm" template="Sunarc_Splitorderpro::splitorder.phtml" />
        </referenceContainer>
         <referenceBlock name="page.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Assign Splitorderpro Attribute</argument>
            </action>
        </referenceBlock>
    </body>
</page>
